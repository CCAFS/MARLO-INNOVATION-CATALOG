# .env.example
# Public API endpoint configuration
PUBLIC_API=your-api-endpoint.com/api

# Cloudflare Turnstile CAPTCHA Configuration
# Get your keys from: https://dash.cloudflare.com/

# Public site key (used in client-side code)
PUBLIC_TURNSTILE_SITE_KEY=your_site_key_here

# Secret key (used for server-side verification - keep private!)
TURNSTILE_SECRET_KEY=your_secret_key_here

# Google Analytics Configuration
GA_ID=your_ga_id_here

# CLARITY Configuration
CLARITY_ID=your_clarity_id_here